PRIVATE_KEY=

# RPC URLs
POLYGON_RPC_URL=https://polygon-rpc.com
MUMBAI_RPC_URL=https://rpc-mumbai.maticvigil.com

# API Keys
POLYGONSCAN_API_KEY=
PINATA_API_KEY=
PINATA_SECRET_API_KEY=

# Database
DATABASE_URL=

# JWT Secret
JWT_SECRET=

# IPFS Gateway
IPFS_GATEWAY=https://gateway.pinata.cloud

# Lit Protocol
LIT_NETWORK=cayenne